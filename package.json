{
  "name": "ember-cli-deploy-minio",
  "version": "0.0.1",
  "description": "A ember-deploy addon to publish files to minio s3",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trustvox/ember-cli-deploy-minio.git"
  },
  "license": "MIT",
  "author": {
    "name": "Bruno Casali",
    "email": "brunoocasali@gmail.com",
    "url": "https://brunoocasali.xyz"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
