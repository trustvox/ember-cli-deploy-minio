    "ember-cli-deploy-plugin": "^0.2.9",
    "ember-cli-shims": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-plugin-node": "^6.0.1",
    "github": "^0.2.4",
    "glob": "^7.1.3",
    "mocha": "^5.2.0"
